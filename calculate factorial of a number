// factorial code:
#include<stdio.h>

int fac(int n);

int main() {
    int n;
    printf("enter a number: ");
    scanf("%d",&n);
    int result=fac(n);
    printf("%d",result);
    return 0;
}

int fac(int n){
    if(n==1 || n==0){
        return 1;
    }
    else{
        int sum=fac(n-1);
        int sumn=n*sum;
        return sumn;
    }
    return 0;
}
