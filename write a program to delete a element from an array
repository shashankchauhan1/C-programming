// write a program to delete a element from array


#include<stdio.h>
int main(){
    int n;
    printf("Enter size of the array: ");
    scanf("%d",&n);
    int arr[n];
    printf("Enter array elements: ");
    for(int i=0; i<n; i++){
        scanf("%d",&arr[i]);
    }
    
    int d;
    printf("Enter the number you want to delete: ");
    scanf("%d",&d);
    int newSize=0;
    for(int i=0; i<n; i++){
        if(arr[i]==d){
            continue;
        }
        arr[newSize]=arr[i];
        newSize++;
    }
    
    for(int i=0; i<newSize; i++){
        printf("%d ",arr[i]);
    }
    
    return 0;
}
