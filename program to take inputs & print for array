// output will be 2 4 6 8 10

#include<stdio.h>
void main() {
    int arr[20],i;
    
    for(int i=0; i<5; i++){
        printf("enter array elements: ");
        scanf("%d",&arr[i]);
    }  

    for(int i=0; i<5; i++){
        arr[i]=arr[i]*2;
        printf("%d\n",arr[i]);
    }
}
