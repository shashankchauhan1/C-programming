// output will be 1 2 3 4 5

#include<stdio.h>
void main() {
    int arr[20],i;

    for(int i=0; i<5; i++){
        printf("enter array elements: ");
        scanf("%d",&arr[i]);
    }  
                                                       
    for(int i=0; i<5; i++){
        printf("%d ",arr[i]);
    }
}



// output will be 2 4 6 8 10

#include<stdio.h>
void main() {
    int arr[20],i;
    
    for(int i=0; i<5; i++){
        printf("enter array elements: ");
        scanf("%d",&arr[i]);
    }  

    for(int i=0; i<5; i++){
        arr[i]=arr[i]*2;
        printf("%d\n",arr[i]);
    }
}




// output will be 3 5 7 9 11

#include<stdio.h>
void main() {
    int arr[20],i;
    
    for(int i=0; i<5; i++){
        printf("enter array elements: ");
        scanf("%d",&arr[i]);
    }  

    for(int i=0; i<5; i++){
        arr[i]=arr[i]+i+2;
        printf("%d ",arr[i]);
    }
}


// program to take input of 5 subject(1 to 5) and give their output also.

#include<stdio.h>
int main() {
    int marks[5],i;
    // input
    for(i=1; i<=5; i++){
        printf("enter marks of sub %d: ",i);
        scanf("%d",&marks[i]);
    }
    // output
    for(i=1; i<=5; i++){
        printf("marks in sub %d = %d\n",i,marks[i]);
    }
    return 0;
}
