 // write a function to calculate the table of the number given by user.

#include<stdio.h>
void table(int n);

int main(){
    int n;
    printf("enter a number: ");
    scanf("%d",&n);
    table(n);
    return 0;
}

void table(int n){
    for(int i=1; i<=10; i++){
        printf("%d * %d = %d\n",n,i,n*i);
    }
}


// without function.

#include<stdio.h>
int main() {
    int n;
    printf("enter a number: ");
    scanf("%d",&n);
    for(int i=1; i<=10; i++){
        printf("%d * %d = %d\n",n,i,n*i);
    }
    return 0;
}	



// write a program to print odd numbers from 5 to 50.

#include<stdio.h>
int main() {
    for(int i=5; i<=50; i++){
        if(i%2==0){
            continue;
        }
        printf("%d\n",i);
    }
    return 0;
}
