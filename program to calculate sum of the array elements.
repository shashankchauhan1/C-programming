// write a program to calculate the sum of the array elements.

#include<stdio.h>
int main() {
    int arr[]={5,6,3,8,9,5,3};
    int sizea=sizeof(arr);
    int sizei=sizeof(int);
    int n=sizea/sizei;
    int sum=0;
    for(int i=0; i<n; i++){
        sum=sum+arr[i];
    }
    printf("%d",sum);
    return 0;
}


// by taking input from user

#include<stdio.h>
int main() {
    int arr[100];
    int n,sum=0;
    printf("enter array size: ");
    scanf("%d",&n);
    printf("enter array elements: \n");
    for(int i=0; i<n; i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0; i<n; i++){
        sum=sum+arr[i];
    }
    printf("%d",sum);
    
    return 0;
}
