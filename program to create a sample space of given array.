// program to create a sample space of given array.

#include<stdio.h>
int main() {
    int arr1[100];
    int arr2[100];
    int n,i,j;
    printf("enter size of array: ");
    scanf("%d",&n);
    printf("enter array elements: ");
    for(int i=0; i<n; i++){
        scanf("%d",&arr1[i]);
    }
    
    for(int i=0; i<n; i++){
            arr2[i]=arr1[i];
    }
    
    for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
            printf("%d%d ",arr1[i],arr2[j]);
        }
        printf("\n");
    }
    
    return 0;
}
