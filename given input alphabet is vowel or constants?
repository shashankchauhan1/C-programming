// check weather the given input alphabet is vowel or constants?

#include<stdio.h>
int main() {
    char ch;
    printf("enter a alphabet: ");
    scanf("%c",&ch);
    if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u'){
        printf("given input is a vowel");
    }
    else{
        printf("given input is a constant");
    }
    return 0;
}
