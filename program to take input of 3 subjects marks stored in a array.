// Q write a program to take input of 3 subjects marks stored in a array.

#include<stdio.h>
int main() {
    int marks[3];						// array initialized of size 3
    
    printf("physics marks: ");
    scanf("%d",&marks[0]);					// adding value by indexes
    
    printf("chemistry marks: ");
    scanf("%d",&marks[1]);
    
    printf("maths marks: ");
    scanf("%d",&marks[2]);
    
    printf("physics: %d, chemistry: %d, maths: %d",marks[0],marks[1],marks[2]);	            // printing output
    return 0;
}
