// program to calculate the perimeter of the rectangle.(formula: 2*(length+breadth) ).

#include<stdio.h>
int main(){
    int l,b;
    printf("enter length: ");
    scanf("%d",&l);
    printf("enter breadth: ");
    scanf("%d",&b);
    printf("perimeter of the rectangle is %d.",2*(l+b));
    return 0;
}
