// fibonaaci series
#include<stdio.h>

int fibo(int n);

int main() {
    int n;
    printf("enter a number: ");
    scanf("%d",&n);
    fibo(n);
    return 0;
}

int fibo(int n){
    int x=0,y=1,z=0;
    while(z<=n){
        printf("%d\n",z);
        x=y;
        y=z;
        z=x+y;
    }
    return 0;
}
