// program for checking hexadecimal character.(0-9,A-F)

#include<stdio.h>
#include<ctype.h>
int main() {
    char ch;
    printf("enter a character: ");
    scanf("%c",&ch);
    if(isxdigit(ch)){
        printf("it is a hexadecimal character.");
    }
    else{
        printf("it is not a hexadecimal character.");
    }
    return 0;
}
