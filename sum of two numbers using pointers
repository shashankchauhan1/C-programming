// sum of 2 numbers using pointers

#include<stdio.h>
int main() {
    int a=4,b=5,*p,*q;
    p=&a;
    q=&b;
    printf("sum: %d",*p+*q);
    return 0;
}


// simple code of pointers


#include<stdio.h>
int main() {
    int a, *p, *q;
    p=&a;
    a=8;
    printf("%d\n",*p);
    *p=22;
    *p=*p+3;
    q=&a;
    printf("%d\n",a);
    printf("%d\n",*q);
    return 0;
}
