// program to reverse the string by swapping.

#include<stdio.h>
#include<string.h>

int reverse(char name[]);

int main() {
    char name[]="shashank";
    reverse(name);
    return 0;
}

int reverse(char name[]){
    int len=strlen(name);
    for(int i=0; i<len/2; i++){
        int temp=name[i];
        name[i]=name[len-i-1];
        name[len-i-1]=temp;
    }
    printf("%s",name);
}
