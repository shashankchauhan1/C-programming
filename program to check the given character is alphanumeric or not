// write a program to check weather the given character is alphanumeric or not(A-Z)(a-z)(0-9).

#include<stdio.h>
#include<ctype.h>
int main() {
    char ch;
    printf("enter a character: ");
    scanf("%c",&ch);
    if(isalnum(ch)){
        printf("it is a alphanumeric");
    }
    else{
        printf("it is not a alphanumeric");
    }
    return 0;
}
